    function initMap() {
        var uluru = {lat: 43.07180049999999, lng: -89.40345990000003};
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 16,
            center: uluru
        });
        var marker = new google.maps.Marker({
            position: uluru,
            map: map
        });
    }
</script>
